{
  "name": "yaddb",
  "version": "0.0.3",
  "description": "Yet Another Dynamo Data Base Library",
  "main": "src/yaddb.js",
  "scripts": {
    "dynamodb": "docker run -p 8000:8000 amazon/dynamodb-local ",
    "test": "NODE_ENV=test jest",
    "pretty": "prettier --write **/*.js",
    "lint": "eslint . --ext .js"
  },
  "author": "devops@labrador.co.uk",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4"
  },
  "dependencies": {
    "aws-sdk": "^2.658.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run pretty",
      "npm run lint",
      "npm run test",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labradorcouk/yaddb.git"
  },
  "keywords": [
    "dynamodb",
    "aws",
    "client",
    "dynamo",
    "amazon"
  ],
  "bugs": {
    "url": "https://github.com/labradorcouk/yaddb/issues"
  },
  "homepage": "https://github.com/labradorcouk/yaddb#readme"
}
